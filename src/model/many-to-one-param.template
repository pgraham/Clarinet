// Populuate the ${rhs} parameter
$rhs = $model->get${lhs_property}();
if ($rhs !== null) {
  $rhsId = $rhs->get${rhs_id_property}();
  if ($rhsId === null) {
    $persister = ActorFactory::getActor('persister', '${rhs}');
    $rhsId = $persister->create($rhs);
  }
} else {
  $rhsId = null;
}
$params[':${lhs_column}'] = $rhsId;
